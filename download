function getOS() {
if (navigator.appVersion.indexOf("Mac")!=-1)
return "mac";
if (navigator.appVersion.indexOf("X11")!=-1 || navigator.appVersion.indexOf("Linux")!=-1)
return "linux";
return "win";
};
});
</script>
