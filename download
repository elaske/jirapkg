<script>
$(function() {
$('#allDownloads').hide();
$('#allDownloadsExpander').css({'cursor':'pointer'}).toggle(function() {
$('#allDownloads').slideDown(100);
$('span.arrow').addClass('open');
}, function() {
$('#allDownloads').slideUp(100);
$('span.arrow').removeClass('open');
});
$('.faq-wrap:nth-child(2)').css({'float':'right', 'clear':'none'});
function scrollTo(id){
$('html,body').animate({scrollTop: $(id).offset().top},300);
}
$('.mac .dialog a').click(function() {
if ($('#allDownloads').is(':visible')) {
scrollTo('#allDownloads');
} else {
$('#allDownloadsExpander').click();
